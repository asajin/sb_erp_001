<h1>TimePrice</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Local_price</th>
            <td>{{ entity.localprice }}</td>
        </tr>
        <tr>
            <th>Currency_price</th>
            <td>{{ entity.currencyprice }}</td>
        </tr>
        <tr>
            <th>Currency_rate</th>
            <td>{{ entity.currencyrate }}</td>
        </tr>
        <tr>
            <th>Stock</th>
            <td>{{ entity.stock }}</td>
        </tr>
        <tr>
            <th>Price_date</th>
            <td>{{ entity.pricedate|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Created_at</th>
            <td>{{ entity.createdat|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Updated_at</th>
            <td>{{ entity.updatedat|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('timeprice') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('timeprice_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('timeprice_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
