<h1>TimePrice list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Local_price</th>
            <th>Currency_price</th>
            <th>Currency_rate</th>
            <th>Stock</th>
            <th>Price_date</th>
            <th>Created_at</th>
            <th>Updated_at</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('timeprice_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.localprice }}</td>
            <td>{{ entity.currencyprice }}</td>
            <td>{{ entity.currencyrate }}</td>
            <td>{{ entity.stock }}</td>
            <td>{% if entity.pricedate %}{{ entity.pricedate|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{% if entity.createdat %}{{ entity.createdat|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{% if entity.updatedat %}{{ entity.updatedat|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('timeprice_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('timeprice_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('timeprice_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
